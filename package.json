{
  "private": false,
  "name": "kelas-rumah-berbagi",
  "description": "Kelas Rumah Berbagi",
  "license": "MIT",
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "scripts": {
    "build": "npm run build:css && npm run build:remix",
    "build:css": "tailwindcss --minify -o ./app/tailwind.css",
    "build:remix": "remix build --sourcemap",
    "dev": "concurrently -k \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "test": "playwright test",
    "pretest:e2e:run": "npm run build",
    "test:e2e:dev": "cross-env RUNNING_E2E=true start-server-and-test dev http-get://localhost:3000/ test",
    "test:e2e:run": "cross-env RUNNING_E2E=true DATABASE_URL=file:./test.db start-server-and-test start http-get://localhost:3000/ test",
    "deploy": "flyctl deploy"
  },
  "dependencies": {
    "@headlessui/react": "1.4.2",
    "@heroicons/react": "1.0.5",
    "@prisma/client": "3.6.0",
    "@reach/auto-id": "0.16.0",
    "@remix-run/react": "1.1.1",
    "@remix-run/serve": "1.1.1",
    "@types/validator": "13.7.0",
    "burner-email-providers": "1.0.67",
    "clsx": "1.1.1",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remix": "1.1.1",
    "remix-auth": "2.5.0-0",
    "validator": "13.7.0"
  },
  "devDependencies": {
    "@playwright-testing-library/test": "4.0.1",
    "@playwright/test": "1.17.1",
    "@remix-run/dev": "1.1.1",
    "@tailwindcss/forms": "0.3.4",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "concurrently": "6.4.0",
    "esbuild-register": "3.2.0",
    "msw": "0.36.3",
    "prettier": "2.5.1",
    "prisma": "3.6.0",
    "start-server-and-test": "1.14.0",
    "tailwindcss": "2.2.19",
    "typescript": "4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}
